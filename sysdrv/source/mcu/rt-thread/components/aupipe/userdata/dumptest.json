{
    "pipeline":{
        "rate":48000,
        "bits":16,
        "period-size":512,
        "filesrc0":{
            "factory":"filesrc",
            "filename":"in.wav",
            "channels":2,
            "sinks":{
                "sink.0":{
                    "name":"queue0",
                    "bypass":true,
                    "sink_ch":2
                }
            }
        },
        "queue0":{
            "factory":"queue",
            "channels":2,
            "buffer-num":20,
            "sinks":{
                "sink.0":{
                    "name":"audiotee0",
                    "bypass":true,
                    "sink_ch":2
                }
            }
        },
        "audiotee0":{
            "factory":"audiotee",
            "channels":2,
            "sinks":{
                "sink.0":{
                    "name":"filesink0",
                    "bypass":false,
                    "sink_ch":2,
                    "binding.0":0,
                    "binding.1":1
                },
                "sink.1":{
                    "name":"audiosink0",
                    "bypass":false,
                    "sink_ch":2,
                    "binding.2":0,
                    "binding.3":1
                }
            }
        },
        "filesink0":{
            "factory":"filesink",
            "filename":"/ramfs/out.raw",
			"channels":2
        },
        "audiosink0":{
            "factory":"audiosink",
            "card":"acodecp",
            "channels":2
        }
    }
}